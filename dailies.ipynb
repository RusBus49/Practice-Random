    "\n",
    "c_temp =  float(input('What is the temperature in celcius?'))\n",
    "\n",
    "def celcius_to_fahrenheit(c_temp):\n",
    "  f_temp = (c_temp*(9/5))+32\n",
    "  \n",
    "  return f_temp\n",
    "\n",
    "print(celcius_to_fahrenheit(c_temp))"
